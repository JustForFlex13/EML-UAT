<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Nb_jours_depuis_Creation_Compte__c</fullName>
    <description>Resitue le nombre de jours écoulés depuis la création du compte. Utilisé pour les filtres dans les rapports.</description>
    <externalId>false</externalId>
    <formula>Now() - CreatedDate</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Nb jours depuis Création Compte</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
