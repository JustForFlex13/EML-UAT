<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Anniversaire_dans_les_30_jours__c</fullName>
    <externalId>false</externalId>
    <formula>AND( 
TODAY()&gt;=(DATE(YEAR(TODAY()),MONTH(Birthdate),DAY(Birthdate))-30), 
TODAY()&lt;=DATE(YEAR(TODAY()),MONTH(Birthdate),DAY(Birthdate)) 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Anniversaire dans les 30 jours</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
