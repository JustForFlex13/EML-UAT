    <rules>
        <fullName>CONVSTA relance PJ</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.Convsta_TypeContrat__c</field>
            <operation>notEqual</operation>
            <value>Convention de stage de l ecole</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>Validation d&apos;expérience</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Convsta_Convention__c</field>
            <operation>equals</operation>
            <value>Faux</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Convsta_Send_email_relance_PJ__c</field>
            <operation>equals</operation>
            <value>Vrai</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Convsta_Document_a_verifier__c</field>
            <operation>equals</operation>
            <value>Faux</value>
        </criteriaItems>
        <description>Cette règle permet de relancer les étudiants n&apos;ayant pas fourni de pièce jointe lors de la création d&apos;une demande de stage.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>CONVSTA_Deuxieme_relance_contrat</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>CONVSTA_Premiere_relance_contrat</name>
                <type>Alert</type>
            </actions>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Convsta Attestation Courte durée - Demande d%27upload</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Case.Convsta_TypeContrat__c</field>
            <operation>equals</operation>
            <value>Contrat local a l etranger</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Convsta_DateEnd__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>Convention signée,Expérience validée</value>
        </criteriaItems>
        <description>Envoi d&apos;une demande d&apos;upload d&apos;attestation pour les &quot;CDD, Interim, Contrat local à l’étranger&quot;</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Attestation_Courte_duree_Demande_d_upload</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Case.Convsta_DateEnd__c</offsetFromField>
            <timeLength>-15</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Convsta Attestation Courte durée - Relance J%2B7</fullName>
        <active>false</active>
        <description>Envois de relances à J+7 de la date de fin pour les &quot;CDD, Interim, Contrat local à l’étranger&quot;</description>
        <formula>( TEXT( Convsta_TypeContrat__c ) = &quot;Contrat de travail de l&apos;entreprise&quot; || TEXT( Convsta_TypeContrat__c ) = &apos;Contrat local a l etranger&apos; ) &amp;&amp; Convsta_Document_a_verifier__c = false &amp;&amp; Convsta_Boolean_Evaluation__c = false &amp;&amp; NOT(ISBLANK(Convsta_DateEnd__c)) &amp;&amp; ( TEXT ( Status ) = &apos;Convention signée&apos; || TEXT ( Status )= &apos;Expérience validée&apos;) &amp;&amp; Convsta_DateEnd__c -TODAY()&gt;=-7</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Attestation_Courte_duree_Relance_Jplus7</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Case.Convsta_DateEnd__c</offsetFromField>
            <timeLength>7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
