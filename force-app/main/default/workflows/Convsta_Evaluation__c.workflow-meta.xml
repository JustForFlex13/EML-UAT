    <rules>
        <fullName>Convsta Evaluation présentation %26 envoi</fullName>
        <active>true</active>
        <description>Envois d&apos;explication et de formulaire de l&apos;évaluation à J-28 et J-21</description>
        <formula>NOT(ISBLANK(DateEndStage__c))  &amp;&amp;  NOT( ISPICKVAL( Demande__r.Convsta_TypeContrat__c , &quot;CDI&quot;) )</formula>
        <triggerType>onCreateOnly</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Convsta_Etudiant_presentation</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Convsta_Evaluateur_presentation</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Convsta_Evaluation__c.DateEndStage__c</offsetFromField>
            <timeLength>-28</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Convsta_Etudiant_evaluation_disponible</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Convsta_Evaluation__c.DateEndStage__c</offsetFromField>
            <timeLength>-21</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
